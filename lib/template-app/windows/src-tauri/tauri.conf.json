{
  "productName": "{{APP_NAME}}",
  "version": "{{APP_VERSION}}",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devUrl": "{{WEBAPP_URL}}",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "{{APP_TITLE}}",
        "width": {{WEBAPP_WIDTH}},
        "height": {{WEBAPP_HEIGHT}},
        "resizable": {{WEBAPP_RESIZABLE}},
        "fullscreen": false,
        "decorations": {{WEBAPP_DECORATIONS}},
        "visible": true
      }
    ]
  },
  "identifier": "{{BUNDLE_IDENTIFIER}}",
  "bundle": {
    "active": true,
    "targets": ["msi"],
    "icon": ["icons/icon.png"],
    "windows": {
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "allowDowngrades": true,
      "webviewInstallMode": {
        "type": "downloadBootstrapper",
        "silent": true
      }
    }
  },
  "plugins": {}
}
